#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    float q[7] = {1,2,5,10,20,50,100},j,R;
    float v=0.009,d=19.1*10,y;
    for(int j = 0; j < 7; ++j) 
  {
      q[j] = q[j]*d/v;
      R = q[j];
      
  	if(R > 9.2*100000)  
      {
        y = ( 0.0021) / (pow(d,0.3));
        printf(" R = %9.6f    y=%.6f  \n",R,y); 
      }
      
    if(R < 9.2*100000)
        y = ( 1,5*0.000001) / (d)+1/q[2];
        y= pow( y,0.3);
        printf(" R = %9.6f    y=%.6f  \n",R,y); 
  }
  
  return 0;
}

